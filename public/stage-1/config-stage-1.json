      }
    },
    "stock-e-cluster": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "cluster",
        "initial_selection": ["JKHY"]
      }
    },
    "introduction": {
      "type": "markdown",
      "path": "stage-1/introduction.md",
      "response": [
        {
          "id": "prolificId",
          "prompt": "Please enter your Prolific ID",
          "required": true,
          "location": "belowStimulus",
          "type": "shortText",
          "placeholder": "Prolific ID",
          "paramCapture": "PROLIFIC_PID"
        }
      ]
    },
    "consent": {
      "type": "markdown",
      "path": "stage-1/consent_stage1.md",
      "nextButtonText": "Agree",
      "response": [
        {
          "id": "accept",
          "prompt": "Do you consent to the study and wish to continue?",
          "required": true,
          "requiredValue": "yes",
          "location": "belowStimulus",
          "type": "radio",
          "options": [
            {
              "label": "Decline",
              "value": "no"
            },
            {
              "label": "Accept",
              "value": "yes"
            }
          ]
        }
      ]
    },
    "post-study-quant": {
      "type": "questionnaire",
      "response": [
        {
          "id": "stocks-familiarity",
          "type": "likert",
          "location": "belowStimulus",
          "prompt": "How familiar are you with stock market concepts and investing?",
          "required": true,
          "leftLabel": "Not at all familiar",
          "rightLabel": "Extremely familiar",
          "numItems": 7
        },
        {
          "id": "comfortable-numbers",
          "type": "likert",
          "location": "belowStimulus",
          "prompt": "How comfortable do you feel interpreting numbers, percentages, and basic statistics?",
          "required": true,
          "leftLabel": "Not at all comfortable",
          "rightLabel": "Extremely comfortable",
          "numItems": 7
        },
        {
          "id": "confident-visuals",
          "type": "likert",
          "location": "belowStimulus",
          "prompt": "How confident are you in your ability to accurately interpret charts, graphs, and other visualizations?",
          "required": true,
          "leftLabel": "Not at all confident",
          "rightLabel": "Extremely confident",
          "numItems": 7
        }
      ]
    }
  },
  "sequence": {
    "order": "fixed",
    "components": [
      "introduction",
      "consent",
      {
        "order": "latinSquare",
        "numSamples": 1,
        "components": [
          {
            "order": "random",
            "components": ["stock-a-control", "stock-b-random", "stock-c-stats", "stock-d-percentileClosest", "stock-e-cluster"]
          },
          {
            "order": "random",
            "components": ["stock-b-control", "stock-c-random", "stock-d-stats", "stock-e-percentileClosest", "stock-a-cluster"]
          },
          {
            "order": "random",
            "components": ["stock-c-control", "stock-d-random", "stock-e-stats", "stock-a-percentileClosest", "stock-b-cluster"]
          },
          {
            "order": "random",
            "components": ["stock-d-control", "stock-e-random", "stock-a-stats", "stock-b-percentileClosest", "stock-c-cluster"]
          },
          {
            "order": "random",
            "components": ["stock-e-control", "stock-a-random", "stock-b-stats", "stock-c-percentileClosest", "stock-d-cluster"]
          }
        ]
      },
      "post-study-quant"
    ]
  }
}
