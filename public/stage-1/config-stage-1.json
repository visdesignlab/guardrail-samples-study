{
  "$schema": "https://raw.githubusercontent.com/revisit-studies/study/dev/src/parser/StudyConfigSchema.json",
  "studyMetadata": {
    "title": "Stage 1",
    "version": "pilot",
    "authors": ["VDL"],
    "date": "2024-01-11",
    "description": "A study challenging participants to review linecharts.",
    "organizations": ["University of Utah"]
  },
  "uiConfig": {
    "contactEmail": "abrar.nadib@utah.edu",
    "logoPath": "assets/revisitLogoSquare.svg",
    "withProgressBar": true,
    "autoDownloadStudy": false,
    "withSidebar": false,
    "urlParticipantIdParam": "PROLIFIC_PID",
    "studyEndMsg": "**Thank you for completing the study. You may click this link and return to Prolific**: [Add your link](Add your link)",
    "enumerateQuestions": true,
    "stylesheetPath": "stage-1/assets/form.css"
  },
  "baseComponents": {
    "stock" : {
            "instruction": "On Jan 1, 2024, a financial advisor told you that the stock highlighted below is one of the best investments you can make. The visualization depicts how it panned out. Please review the chart and answer the questions below.",
            "instructionLocation": "aboveStimulus",
            "type": "react-component",
            "path": "viz-guardrails/DataExplorer.tsx",
            "parameters": {
                "dataset": "sp500_stocks",
                "start_date": "2024-01-01",
                "end_date": "2024-12-31",
                "allow_time_slider": false,
                "allow_guardrail_selector": false,
                "allow_selection": false,
                "allow_help": false,
                "guardrail": "super_summ",
                "caption": "",
                "x_var": "date",
                "y_var": "value",
                "cat_var": "name",
                "group_var": "region"
            },
            "nextButtonLocation": "belowStimulus",
            "response": [
                {
                  "id": "investment-likelihood",
                  "type": "likert",
                  "location": "belowStimulus",
                  "prompt": "Imagine the advisor gave you a free welcome bonus of $100 to invest. Based on the visualization shown above, how likely are you to choose the highlighted stock for your investment?",
                  "numItems": 7,
                  "rightLabel": "Extremely likely",
                  "leftLabel": "Not at all likely",
                  "required": true
                },
                {
                    "id": "accuracy",
                    "type": "slider",
                    "location": "belowStimulus",
                    "prompt": "How well did this stock perform compared to all other stocks in the market? Drag the slider to answer (1 means 'worst' and 100 means 'best ')",
                    "options": [
                        {
                            "value": 1,
                            "label": "1"
                        },
                        {
                            "value": 25,
                            "label": "25"
                        },
                        {
                            "value": 50,
                            "label": "50"
                        },
                        {
                            "value": 75,
                            "label": "75"
                        },
                        {
                            "value": 100,
                            "label": "100"
                        }
                    ],
                    "required": true,
                    "step": 1

                },
                {
                    "id": "completeness",
                    "type": "likert",
                    "location": "belowStimulus",
                    "prompt": "Do you feel that the visualization provided appropriate context to make an informed decision about your investment?",
                    "numItems": 7,
                    "leftLabel": "Not at all",
                    "rightLabel": "Definitely",
                    "required": true
                }
            ]
        }
  },
  "components": {
    "stock-a-control": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "none",
        "initial_selection": ["VZ"]
      }
    },
    "stock-a-random": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "super_data",
        "initial_selection": ["VZ"]
      }
    },
    "stock-a-stats": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentiles",
        "initial_selection": ["VZ"]
      }
    },
    "stock-a-percentileClosest": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentileClosest",
        "initial_selection": ["VZ"]
      }
    },
    "stock-a-cluster": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "cluster",
        "initial_selection": ["VZ"]
      }
    },
    "stock-b-control": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "none",
        "initial_selection": ["COR"]
      }
    },
    "stock-b-random": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "super_data",
        "initial_selection": ["COR"]
      }
    },
    "stock-b-stats": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentiles",
        "initial_selection": ["COR"]
      }
    },
    "stock-b-percentileClosest": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentileClosest",
        "initial_selection": ["COR"]
      }
    },
    "stock-b-cluster": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "cluster",
        "initial_selection": ["COR"]
      }
    },
    "stock-c-control": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "none",
        "initial_selection": ["CHD"]
      }
    },
    "stock-c-random": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "super_data",
        "initial_selection": ["CHD"]
      }
    },
    "stock-c-stats": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentiles",
        "initial_selection": ["CHD"]
      }
    },
    "stock-c-percentileClosest": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentileClosest",
        "initial_selection": ["CHD"]
      }
    },
    "stock-c-cluster": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "cluster",
        "initial_selection": ["CHD"]
      }
    },
    "stock-d-control": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "none",
        "initial_selection": ["TEL"]
      }
    },
    "stock-d-random": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "super_data",
        "initial_selection": ["TEL"]
      }
    },
    "stock-d-stats": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentiles",
        "initial_selection": ["TEL"]
      }
    },
    "stock-d-percentileClosest": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentileClosest",
        "initial_selection": ["TEL"]
      }
    },
    "stock-d-cluster": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "cluster",
        "initial_selection": ["TEL"]
      }
    },
    "stock-e-control": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "none",
        "initial_selection": ["JKHY"]
      }
    },
    "stock-e-random": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "super_data",
        "initial_selection": ["JKHY"]
      }
    },
    "stock-e-stats": {
      "baseComponent": "stock",
      "parameters": {
        "guardrail": "percentiles",
